# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_anon_key

# Import Scripts Configuration
# The service role key is needed to bypass RLS during import. 
# Get it from `supabase status` or the Supabase dashboard.
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# The UUID of the user who will own the imported routes.
IMPORT_USER_ID=your_user_id_here
